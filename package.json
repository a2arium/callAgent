{
  "name": "callagent",
  "version": "0.2.0",
  "description": "AI Agents Framework with A2A Support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "postinstall": "node scripts/sync-dotenv.cjs",
    "build": "tsc",
    "start": "node dist/runner/index.js",
    "dev": "node --loader ts-node/esm src/runner/index.ts",
    "run-agent": "yarn turbo run build && node packages/core/dist/runner/runnerCli.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "run:llm": "yarn run-agent apps/examples/llm-agent/dist/AgentModule.js '{\"prompt\": \"Tell me a joke about AI.\"}'",
    "run:memory": "yarn run-agent apps/examples/memory-usage/dist/AgentModule.js '{}' --stream --tenant=customer-123",
    "clean": "find . -type d -name dist -exec rm -rf {} +"
  },
  "keywords": [
    "ai",
    "agent",
    "framework"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/uuid": "^10.0.0",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "prisma": "^6.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "^6.7.0"
  },
  "workspaces": [
    "packages/*",
    "apps/*",
    "apps/examples/*"
  ],
  "private": true,
  "packageManager": "yarn@1.22.22"
}
